# Google OAuth Client ID (Required for Google Drive Sync)
# Get this from Google Cloud Console > APIs & Services > Credentials
VITE_GOOGLE_CLIENT_ID=your_clients_id_here.apps.googleusercontent.com

# Google Service API Key
VITE_GOOGLE_API_KEY=your_api_key_here

# Development Server Port
# If not specified, defaults to 4444
VITE_PORT=4444

# Preview Server Port
# If not specified, defaults to 4445
VITE_PREVIEW_PORT=4445
